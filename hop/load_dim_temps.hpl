        <jsScript_script>// Extraire la parti date de la clé datetime
date_only = datetime_key.substring(0, 10);

//est_weekend : samedi (6) ou dimanche (7)
if (jour_semaine == 6 || jour_semaine == 7) {
  est_weekend = 1;
} else {
  est_weekend = 0;
}

//est_dejeuner_fin : entre 12h00 et 14h00
if (heure >= 12 &amp;&amp; heure &lt; 14) {
  est_dejeuner_fin = 1;
} else {
  est_dejeuner_fin = 0;
}
</jsScript_script>
